    {
      "cell_type": "code",
      "source": [
        "def calculator():\n",
        "  mnozh1 = set(input().split(' '))\n",
        "  mnozh2 = set(input().split(' '))\n",
        "  operand = input()\n",
        "  \n",
        "  if operand == 'u':\n",
        "    answer = Unity(mnozh1, mnozh2)\n",
        "  elif operand == 'i':\n",
        "    answer = Intersection(mnozh1, mnozh2)\n",
        "  elif operand == 's':\n",
        "    answer = Minus(mnozh1, mnozh2)\n",
        "  elif operand == 'a':\n",
        "    answer = Add(mnozh1, mnozh2)\n",
        "  else:\n",
        "    print('не могу посчитать :(')\n",
        "  \n",
        "  print(sorted(answer))"
      ],
      "metadata": {
        "id": "ZpVuDVlfBIDu"
      },
      "execution_count": 163,
      "outputs": []
    },
