        "Генератор булеанов числового множества"
      ],
      "metadata": {
        "id": "IuMJbGCGinCr"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "def Subsets(A):\n",
        "  sub_A = [[ ]]\n",
        "  for i in A:\n",
        "    sub_A += [s + [i] for s in sub_A]\n",
        "  return sub_A\n",
        "\n",
        "A = set(input().split(' '))\n",
        "mn = Subsets(A)\n",
        "for s in mn:\n",
        "  if len(s):\n",
        "    print(set(s))\n",
        "  else:\n",
        "    print(\"{}\")\n",
        "\n",
        "    "
      ],
      "metadata": {
        "id": "0IaE--cXIkgl",
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "outputId": "c3af6a98-9328-470e-e4dc-6344fd709e06"
      },
      "execution_count": 179,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
